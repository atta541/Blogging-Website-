{
    "version": 2,
    "builds": [
      {
        "src": "Blog/my-new-blog-frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "Blog/my-new-blog-frontend/build"
        }
      },
      {
        "src": "Blog/my_blog_project/manage.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "Blog/my_blog_project/$1"
      },
      {
        "src": "/static/(.*)",
        "dest": "Blog/my_blog_project/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "Blog/my_blog_project/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "Blog/my-new-blog-frontend/build/$1"
      }
    ]
  }
  